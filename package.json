{
  "name": "@dxworks/depinder",
  "version": "0.2.0",
  "description": "",
  "keywords": [
    "dxworks",
    "dxw"
  ],
  "homepage": "https://github.com/dxworks/depinder#readme",
  "bugs": {
    "url": "https://github.com/dxworks/depinder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxworks/depinder.git"
