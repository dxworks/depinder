    "minimatch": "^10.0.0",
    "moment": "^2.30.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.0",
    "node-html-parser": "^7.0.0",
    "npm": "^11.0.0",
    "npm-registry-fetch": "^19.0.0",
    "pom-parser": "^1.2.0",
    "puppeteer": "^24.0.0",
    "semver": "^7.7.0",
    "snyk-gradle-plugin": "^5.1.0",
    "snyk-nodejs-lockfile-parser": "^2.0.0",
    "spdx-correct": "^3.1.1",
    "spdx-license-ids": "^3.0.10",
    "tmp": "^0.2.1",
    "toml": "^3.0.0",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/dockerode": "^4.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^24.0.0",
    "@types/npm-registry-fetch": "^8.0.0",
    "@types/semver": "^7.7.0",
    "@types/spdx-correct": "^3.1.0",
    "@types/string-template": "^1.0.0",
    "@types/tmp": "^0.2.0",
    "@types/ws": "^8.18.0",
    "@types/xml2js": "^0.4.0",
    "@eslint/js": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.0.0",
    "i": "^0.3.7",
    "jest": "^29.0.0",
    "json": "^11.0.0",
    "performance-now": "^2.1.0",
    "rimraf": "^6.0.0",
    "sort-package-json": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dxw": {
    "commands": [
      {
        "file": "dist/depinder.js",
        "command": "mainCommand"
      }
    ]
  }
